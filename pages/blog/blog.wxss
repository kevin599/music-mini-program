  margin: 0 1%;
  
}
.cube-box{
  perspective: 1600rpx;  
}
.cube{
  width: 80rpx;
  height: 80rpx;
  margin: 60rpx auto 60rpx 8%;
  transform-style: preserve-3d;
  animation: rotate 7s infinite linear;
}
.cube>view{
  width: 80rpx;
  height: 80rpx;
  /* opacity: 0.7; */
  position: absolute;
}
.cube view image{
  width: 80rpx;
  height: 80rpx;
}
.box1{
  transform: rotateY(90deg) translateZ(40rpx);
}
.box2{
  transform: rotateY(-90deg) translateZ(40rpx);
}
.box3{
  transform: rotateX(90deg) translateZ(40rpx);
}
.box4{
  transform: rotateX(-90deg) translateZ(40rpx);
}
.box5{
  transform: translateZ(40rpx);
}
.box6{
  transform: rotateY(180deg) translateZ(40rpx);
}
@keyframes rotate {
  form{
    transform: rotateX(0) rotateZ(0);
  }
  to{
    transform: rotateX(1turn) rotateZ(1turn);
  }
}
